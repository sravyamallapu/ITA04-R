# Generate a random sample from LETTERS
set.seed(123)  # Set seed for reproducibility
random_sample <- sample(LETTERS, size = 20, replace = TRUE)

# Create a factor from the random sample
letter_factor <- factor(random_sample)

# Display the factor levels
cat("All factor levels:\n")
print(levels(letter_factor))

# Extract the first five levels
five_levels <- levels(letter_factor)[1:5]

# Display the extracted five levels
cat("\nExtracted five levels:\n")
print(five_levels)
